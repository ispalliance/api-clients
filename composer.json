{
  "name": "ispa/api-clients",
  "description": "API clients for all ISPA applications",
  "keywords": [
    "ispa",
    "php",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/ispalliance/api-clients",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "contributte/utils": "^0.3.2 || ^0.4.0",
    "guzzlehttp/guzzle": "^6.3.3"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "nette/di": "~2.4.15 || ~3.0.0",
    "ninjify/qa": "^0.9.0",
    "phpstan/phpstan": "^0.11.8",
    "phpstan/phpstan-deprecation-rules": "^0.11.2",
    "phpstan/phpstan-nette": "^0.11.1",
    "phpstan/phpstan-strict-rules": "^0.11.1",
    "phpunit/phpunit": "^8.1.6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ISPA\\ApiClients\\": "src",
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
